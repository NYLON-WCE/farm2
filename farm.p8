function add_kabu(x)
	local s={}
	s.x,s.y,s.t=x,48,0
	function s._u()
	 if s.t<8 then
	  s.t+=1
	  if (s.t==8) shake=1
	 else
 		s.y-=6
	 	if (s.y<-12) del(kabu,s)
	 end
	end
 function s._d()
 	spr(66,s.x-8,s.y-12,2,3)
	end
	add(kabu,s) sfx(2) score+=5*(5-#spnrc)
	add_num(s,50*(5-#spnrc),20)
end
