--effect


function add_ptcl(x,y,c,r)
 local p={}
 p.x,p.y,p.v,p.r,p.t,p.c,p.rr=x,y,1.5,0,16,c,r
 if (p.rr==nil) p.rr=0
 function p._u()
  if (p.t<=0) del(ptcl,p)
  p.r+=p.v p.v*=0.9	p.t-=1
 end
 function p._d()
 	for i=0,7 do
 		circfill(p.x+p.r*cos(i/8),p.y+p.r*sin(i/8),p.rr,p.c)
		end
 end
 add(ptcl,p)
end

function init_cloud()
 cloud={}
 for i=0,10 do
 	local c={}
 	c.x,c.y,c.r=rnd(256),myrnd(8),16+rnd(12)
  function c._u()
  	c.x-=0.25+0.2*sin(t/360)
  	if (c.x<-48) c.x,c.y,c.r=256,myrnd(8),12+rnd(12)
  end
  function c._d()
  	sspr(80,16,32,16,c.x,c.y,c.r*2,c.r)
  end
  add(cloud,c)
 end
end

function add_num(a,n,y)
	local s={}
	s.x,s.y,s.n,s.col,s.t=a.x,a.y,n,1,0
	if (y!=nil) s.y-=y
	function s._u()
		if s.t>16 then
			del(num,s)
		elseif s.t>15 then
		 s.y-=1
		elseif s.t>14 then
		 s.col=15
		end
		s.t+=1
	end
	function s._d()
		print(s.n,s.x,s.y,s.col)
	end
	add(num,s)
end
