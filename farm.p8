function add_shot(x,r)
	local s={}
	s.x,s.y,s.r,s.c=x,48+r,r,1
	function s._u()
		s.y+=6
		if (s.y>140) del(shot,s)
	end
 function s._d()
 	sspr(16,32,16,24,s.x-s.r,s.y-2*s.r,s.r*2,s.r*3)
	end
	add(shot,s) sfx(1)
end
