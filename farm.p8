version=0.5

function _init()
 cartdata("nylon_farmpanic2")
 if version==dget(0) then
  hscore=dget(1)
 else
  hscore=0
 end
 dset(0,version)
	music()
	init_cloud()
	t,title,plnum,charay=0,64,1,0
	init_spnr()
	shake,score,sky,st=0,0,0,0
 ptcl={}
	music(0)
end

function init_game()
	pl={}
	for i=1,plnum do
		add(pl,init_pl(128*i/(plnum+1)))
	end
	init_spnr()
	shot,kabu,enemy,num={},{},{},{}
	shake,score,sky,st=0,0,0,0
	music(0)
end

function _update60()
 if title>-16 then
  if (charay>0) charay-=1
  if (btnp(‚¨ÖÔ∏è)) plnum-=1 charay=3
  if (btnp(‚û°Ô∏è)) plnum+=1 charay=3
  plnum=mid(1,plnum,2)
  if charay==1 then
  	for i=1,plnum do
  		add_ptcl(i*128/(plnum+1),48,7,0)
			end
  end
  if title==64 then
   if btn(üÖæÔ∏è) or btn(‚ùé) then
   	title-=1 sfx(2) charay=0
	   init_game()
   end
  else
   title-=(64-title)
  end
 else
  if #spnrc==0 then
   for i=1,plnum do
    pl[i].s=4
   end
   if (btn(üÖæÔ∏è) or btn(‚ùé)) init_game() sfx(2)
  else
   if (t%60==0) add_enemy()
   for i=1,plnum do
    update_pl(i)
   end
	  st+=1
 	 if (st==1800) st=0 sky+=1 sky%=3
  end
  foreach(shot,update)
  foreach(enemy,update)
  foreach(spnr,update)
  foreach(kabu,update)
  foreach(num,update)
 end
 foreach(ptcl,update)
 foreach(cloud,update)
 t+=1 t%=360
end

function _draw()
 local c=9
 if sky==0 then
  cls(12) c=10
 elseif sky==1 then
 	cls(8)
  pal(7,2) pal(6,1) pal(13,1) pal(11,9)
 else
  cls(0) pal(3,1) pal(11,6) pal(1,13)
  pal(7,1) pal(6,13) pal(13,6)
 end
 circfill(16,16,8,c)
 if sky==2 then circfill(17,15,7,10)
 elseif sky==0 then
  for i=0,11 do
   local theta=i/12+t/360
 	 pset(16.5+12*cos(theta),16.5+12*sin(theta),c)
  end
 end
 foreach(cloud,draw)
 if shake>0.5 then
 	local x,y=myrnd(shake),myrnd(shake)
 	camera(x,y)
 	shake*=0.95
 else
  shake=0
 end
 spr(192,0,16,16,4)
 for i=0,5 do
 	for j=-1,8 do
 		spr(64,16*j,16*i+48,2,2)
		end
 end
 pal() palt(0,false) palt(12,true)
 foreach(spnr,draw)
 foreach(shot,draw)
 foreach(kabu,draw)
 foreach(enemy,draw)
 if title<=-16 then
  for i=1,plnum do
   draw_pl(i)
  end
 else
  for i=1,plnum do
  	spr(8*i-8,i*128/(plnum+1)-8,32-charay,2,2,i<=(plnum+1)/2,false)
  end
 end
 for i=0,3 do
  spr(112,32*i,41,4,1)
 end
 palt()
 if title>-16 then
 	spr(128,6,title-8,16,4)
 	print("\#5start:üÖæÔ∏è or ‚ùé",36,80,6)
 end
 foreach(ptcl,draw)
 foreach(num,draw)
 camera()
 print_score()
 if #spnrc==0 then
 	print("\#0   game  over   \nrestart:üÖæÔ∏è or ‚ùé",33,60,7)
 end
 --print(stat(1),2,8,2)
end

function update(self)
	self:_u()
end

function draw(self)
	self:_d()
end

function myrnd(a)
	return rnd(2*a)-a
end

function frnd(a)
	return flr(rnd(a))
end

function dist(a,b)
	return sqrt((a.x-b.x)^2+(a.y-b.y)^2)
end

function limit(a,b,x,y)
	a[b]=max(x,min(a[b],y))
end

function print_score()
 local s1,s2=tostr(hscore),tostr(score)
 s1,s2=s1.."0",s2.."0"
 for i=1,5-s_len(hscore) do
 	s1="0"..s1
 end
 for i=1,5-s_len(score) do
 	s2="0"..s2
 end
 print("hi-score:"..s1.."    score:"..s2,2,2,5)
end

function s_len(s)
	if s<10 then
	 return 1
	else
		return s_len(s/10)+1
	end
end
