function init_spnr()
 spnr={}
 spnrc={1,2,3,4}
 for i=1,4 do
 	local s={}
	 s.x,s.f,s.r,s.n,s.d=32*i-16,0,2,i,false
	 function s._u()
	  if not s.d then
 	 	if s.f<30 then
	  	 s.f+=1
	  	 if (s.f==30) add_ptcl(s.x,48,7)
	  	end
	  	s.r=2+flr(s.f/5)
	  	for i=1,plnum do
 	  	if s.f>=10 and abs(pl[i].x-s.x)<12 then
	   		if btn(‚¨áÔ∏è,i-1) or btn(‚ùé,i-1) then
	      pl[i].af,pl[i].a=10,2
	      shake=2
	  	   s.f=-90
	  	   add_shot(s.x,s.r)
	  		 elseif s.f==30 and (btn(‚¨ÜÔ∏è,i-1) or btn(üÖæÔ∏è,i-1)) then
   	   pl[i].af,pl[i].a=20,1
	  	   s.f=-90
	  	   add_kabu(s.x)
			 	 end
			 	end
		 	end
   end
  end
  function s._d()
   if s.d then
    spr(36+2*flr(t%60/30),s.x-8,36,2,2)
   else
  	 if s.f>0 then
   		sspr(16,32,16,24,s.x-s.r,48-s.r*1.5,s.r*2,s.r*3)
		 		rectfill(s.x-9,63,s.x+8,65,0)
		 	 local c=8
		 	 if s.f==30 then c=11
		 	 elseif s.f>=10 then c=10 end
		 		line(s.x-8,64,s.x-8+s.f/2,64,c)
		 	end
		 end
  end
  add(spnr,s)
	end
end
